<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
 "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
 <validators>
	<field name="cuentaorigen">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Necesitas seleccionar una cuenta Origen</message>
			<!--<message key="err.DNI"/>  Acceso a archivos multilingues -->
		</field-validator>
	</field>
	<field name="cuentadestino">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			 <message>Necesitas seleccionar una cuenta Destino</message>
			<!--<message key="err.PIN"/>  Acceso a archivos multilingues -->
		</field-validator>
	</field>
	<field name="importe">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			 <message>Necesitas introducir un Importe</message>
			<!--<message key="err.PIN"/> Acceso a archivos multilingues -->
		</field-validator>
	</field>
</validators>